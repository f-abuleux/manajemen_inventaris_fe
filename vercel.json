{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}